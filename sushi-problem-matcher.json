{
  "problemMatcher": [
    {
      "owner": "sushi",
      "pattern": [
        {
          "regexp": "^Sushi: (?>\\x1B\\[[0-9;]*m)*(error|warn)(?>\\x1B\\[[0-9;]*m)* (.+)$",
          "severity": 1,
          "message": 2
        },
        {
          "regexp": "^Sushi:   File: (.+)$",
          "file": 1
        },
        {
          "regexp": "^Sushi:   Line: (\\d+)( - (\\d+))?$",
          "line": 1
        }
      ]
    }
  ]
}
