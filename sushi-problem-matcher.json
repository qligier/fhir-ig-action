{
  "problemMatcher": [
    {
      "owner": "sushi",
      "pattern": [
        {
          "regexp": "^(error|warning) (.+)$",
          "severity": 1,
          "message": 2
        }, {
          "regexp": "^  File: (.+)$",
          "file": 1
        }, {
          "regexp": "^  Line: (.+)$",
          "line": 1
        }
      ]
    }
  ]
}
