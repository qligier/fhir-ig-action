{
  "problemMatcher": [
    {
      "owner": "sushi",
      "pattern": [
        {
          "regexp": "^Sushi: (error|warning) (.+)$",
          "severity": 1,
          "message": 2
        },
        {
          "regexp": "^Sushi:   File: (.+)$",
          "file": 1
        },
        {
          "regexp": "^Sushi:   Line: (.+)$",
          "line": 1
        }
      ]
    }
  ]
}
