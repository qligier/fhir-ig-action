{
  "problemMatcher": [
    {
      "owner": "igpublisher",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^Stack Dump (for debugging):\\s+\\(\\d{2}:\\d{2}\\.\\d{3}\\)$"
        },
        {
          "regexp": "^(.+)$",
          "message": 1
        }
      ]
    }
  ]
}
